@model SHWY.Model.DB.CodeType
@{
    ViewBag.Title = "CodeTypeEdit";
    Layout = "~/Views/Shared/_BaseContent.cshtml";
}
<form id="formCodeTypeEdit">
    <table class="table table-bordered">
        <tr>
            <td>
                类型名称：
            </td>
            <td>@Html.TextBoxFor(p => p.TypeName, new { @class = "easyui-textbox", data_options = "required:true" })</td>
        </tr>
        <tr>
            <td>
                备注：
            </td>
            <td>@Html.TextBoxFor(p => p.Remark, new { @class = "easyui-textbox", data_options = "multiline:true", style = "width:100%;height:50px;" })</td>
        </tr>
    </table>
    @Html.HiddenFor(p => p.TypeId)
</form>
