@model SHWY.Model.DB.ServerIp
@{
    ViewBag.Title = "ServerIpEdit";
    Layout = "~/Views/Shared/_BaseContent.cshtml";
}

<form id="formServerIPEidt">
    <table>
        <tr>
            <td>@Html.LabelFor(p => p.sid)</td>
            <td>@Html.DropDownListFor(p => p.sid, ViewBag.ServerList as IEnumerable<SelectListItem>, new { @class = "easyui-combobox", data_options = "editable:false,readonly:"+ViewBag.isRealOnly + "", style = "width:170px;" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.ipid)</td>
            <td>@Html.DropDownListFor(p => p.ipid, ViewBag.IpList as IEnumerable<SelectListItem>, new { @class = "easyui-combobox", data_options = "editable:false,readonly:" + ViewBag.isRealOnly + "", style = "width:170px;" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.itemid)</td>
            <td>@Html.DropDownListFor(p => p.itemid, ViewBag.ItemList as IEnumerable<SelectListItem>, new { @class = "easyui-combobox", data_options = "editable:false", style = "width:170px;" })</td>
        </tr>
    </table>
</form>

