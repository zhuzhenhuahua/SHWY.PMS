@model SHWY.Model.DB.Servers
@{
    ViewBag.Title = "ServerEdit";
    Layout = "~/Views/Shared/_BaseContent.cshtml";
}
<form id="formServerEidt">
    <table>
        <tr>
            <td>@Html.LabelFor(p => p.partyId)</td>
            <td>@Html.DropDownListFor(p => p.partyId, ViewBag.PartyList as IEnumerable<SelectListItem>, new { @class = "easyui-combobox", data_options = "editable:false", style = "width:170px;" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.name)</td>
            <td>@Html.TextBoxFor(p => p.name, new { @class = "easyui-textbox", data_options = "required:true" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.remark)</td>
            <td>@Html.TextBoxFor(p => p.remark, new { @class = "easyui-textbox", data_options = "multiline:true", style = "width:450px;height:50px;" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.loginName)</td>
            <td>@Html.TextBoxFor(p => p.loginName, new { @class = "easyui-textbox" })</td>
        </tr>
        <tr>
            <td>@Html.LabelFor(p => p.password)</td>
            <td>@Html.TextBoxFor(p => p.password, new { @class = "easyui-textbox" })</td>
        </tr>
    </table>
    @Html.HiddenFor(p => p.sid)
</form>
