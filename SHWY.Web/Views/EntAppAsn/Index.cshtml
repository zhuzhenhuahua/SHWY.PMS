
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="padding-left:300px;padding-top:300px;">
    <table id="tt"></table>
</div>
<script type="text/javascript">
    $(function () {
        BindGridView();
    });
    function BindGridView() {
        $("#tt").datagrid({
            url: '/EntAppAsn/GetList',
            columns: [[
                { field: 'Asn_No', title: 'Asn_No' },
                { field: 'Customer_No', title: 'Customer_No' },
                { field: 'Bill_No_All', title: 'Bill_No_All' },
                { field: 'Fob_Type', title: 'Fob_Type' },
                { field: 'Dozen', title: 'Dozen' },
                { field: 'Package_Type', title: 'Package_Type' }
            ]]
        });
    }
</script>

